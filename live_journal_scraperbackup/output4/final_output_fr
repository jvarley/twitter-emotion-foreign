                });
            

                <!-- Hide from old browsers
                // Cache-busting and pageid values
                if (!pageNum) var pageNum = Math.round(Math.random() * 10000000000);
                document.write('<SCR');
                document.write('IPT LANGUAGE="JavaScript" SRC="http://ad.doubleclick.net/adj/lj.ugc.entry/;'
                    + 'a=1;r=0;w=1;wg=0;wa=99;wc=FR;wi=harry;wi=potter;wi=mercy;wi=thompson;wi=star;wi=wars;wi=william;wi=shakespeare;ra=99;jid=40802656;kw=ofni;kw=song-fic;kw=modena;c=jo;pt=jo;scn=no;' + 'sz=728x90;pos=t;tile=1;ord=' + pageNum + '?" TYPE="text/javascript">');
                document.write('</SCR');
                document.write('IPT>');
                // End Hide -->
                













