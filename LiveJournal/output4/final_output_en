            function fullscreenAShow()
            {
                // ********* CODE START *********
            // Cache-busting and pageid values
            if (!pageNum) var pageNum = Math.round(Math.random() * 10000000000);
            document.write('<SCR');
            document.write('IPT LANGUAGE="JavaScript" SRC="http://ad.doubleclick.net/adj/lj.servicepages/fullscreenadsbml;'
                + 'a=1;r=0;w=1;wg=2;wa=18;wc=US;wi=bowling;wi=for;wi=soup;wi=breaking;wi=benjamin;wi=chris;wi=crutcher;wi=crossfade;wi=desole;wi=eminem;ra=18;rg=2;jid=11603062;c=jo;pt=jo;scn=no;' + 'a=1;r=0;w=0;c=se;pt=se' + ';dcopt=ist;fs=1;sz=1x1,pos=r;tile=1;ord=' + pageNum + '?" TYPE="text/javascript">');
