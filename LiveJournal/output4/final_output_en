            {
                // ********* CODE START *********
            // Cache-busting and pageid values
            if (!pageNum) var pageNum = Math.round(Math.random() * 10000000000);
            document.write('<SCR');
            document.write('IPT LANGUAGE="JavaScript" SRC="http://ad.doubleclick.net/adj/lj.servicepages/fullscreenadsbml;'
                + 'a=1;r=0;w=1;wg=2;wa=20;wc=UK;wac=br;wac=im;wac=s;wi=aragorn;wi=atsuma;wi=balthier;wi=bam;wi=margera;wi=bam;wi=x;wi=ville;wi=basch;wi=battle;ra=20;rg=2;jid=8960854;kw=rant;kw=things;kw=abolish;kw=when;kw=ruler;kw=of;kw=th;c=jo;pt=jo;scn=no;' + 'a=1;r=0;w=0;c=se;pt=se' + ';dcopt=ist;fs=1;sz=1x1,pos=r;tile=1;ord=' + pageNum + '?" TYPE="text/javascript">');
