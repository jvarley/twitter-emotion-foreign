            function fullscreenAShow()
            {
                // ********* CODE START *********
            // Cache-busting and pageid values
            if (!pageNum) var pageNum = Math.round(Math.random() * 10000000000);
            document.write('<SCR');
            document.write('IPT LANGUAGE="JavaScript" SRC="http://ad.doubleclick.net/adj/lj.servicepages/fullscreenadsbml;'
                + 'a=1;r=0;w=1;wg=2;wa=22;wc=CA;wi=antique;wi=cafe;wi=avenged;wi=sevenfold;wi=bam;wi=bif;wi=naked;wi=bonjovi;wi=bullet;wi=for;ra=22;rg=2;jid=9796799;c=jo;pt=jo;scn=yes;' + 'a=1;r=0;w=0;c=se;pt=se' + ';dcopt=ist;fs=1;sz=1x1,pos=r;tile=1;ord=' + pageNum + '?" TYPE="text/javascript">');
            document.write('</SCR');
            document.write('IPT>');
                // ********* CODE FINISH *********
