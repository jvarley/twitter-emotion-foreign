            function fullscreenAShow()
            {
                // ********* CODE START *********
            // Cache-busting and pageid values
            if (!pageNum) var pageNum = Math.round(Math.random() * 10000000000);
            document.write('<SCR');
            document.write('IPT LANGUAGE="JavaScript" SRC="http://ad.doubleclick.net/adj/lj.servicepages/fullscreenadsbml;'
                + 'a=1;r=0;w=1;wg=2;wa=23;wc=RU;wac=br;wac=cp;wac=mo;wac=ah;wac=n;wi=baptise;wi=bible;wi=bonbons;wi=books;wi=chanter;wi=cities;wi=englich;wi=filology;wi=francais;wi=french;ra=23;rg=2;jid=9273821;c=jo;pt=jo;scn=yes;' + 'a=1;r=0;w=0;c=se;pt=se' + ';dcopt=ist;fs=1;sz=1x1,pos=r;tile=1;ord=' + pageNum + '?" TYPE="text/javascript">');
