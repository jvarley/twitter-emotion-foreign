            function fullscreenAShow()
            {
                // ********* CODE START *********
            // Cache-busting and pageid values
            if (!pageNum) var pageNum = Math.round(Math.random() * 10000000000);
            document.write('<SCR');
            document.write('IPT LANGUAGE="JavaScript" SRC="http://ad.doubleclick.net/adj/lj.servicepages/fullscreenadsbml;'
                + 'a=1;r=0;w=1;wg=2;wa=18;wc=CA;wi=anime;wi=art;wi=art;wi=asians;wi=blabbering;wi=cosplay;wi=cosplayers;wi=dessin;wi=deviantart;wi=digital;ra=18;rg=2;jid=28024809;kw=new;kw=year;kw=art;kw=arabs;kw=derpyness.;kw=jews;kw=muslims;c=jo;pt=jo;scn=yes;' + 'a=1;r=0;w=0;c=se;pt=se' + ';dcopt=ist;fs=1;sz=1x1,pos=r;tile=1;ord=' + pageNum + '?" TYPE="text/javascript">');
