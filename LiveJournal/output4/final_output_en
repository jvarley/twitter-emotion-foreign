                // Cache-busting and pageid values
                if (!pageNum) var pageNum = Math.round(Math.random() * 10000000000);
                document.write('<SCR');
                document.write('IPT LANGUAGE="JavaScript" SRC="http://ad.doubleclick.net/adj/lj.ugc.entry/;'
